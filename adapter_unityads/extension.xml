<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.anythink.adapter.unityads</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="default">
			<applicationDeployment>
			</applicationDeployment>
		</platform>
		 <platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>anythinkNetworkUnityads.jar</nativeLibrary>
				<initializer>com.anythink.ane.SDK</initializer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>anythinkNetworkUnityads.jar</nativeLibrary>
				<initializer>com.anythink.ane.SDK</initializer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libATAneUnityAdsAdapter.a</nativeLibrary>
				<initializer>ATUnityAdsExtensionInitializer</initializer>
				<finalizer>ATUnityAdsExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libATAneUnityAdsAdapter.a</nativeLibrary>
				<initializer>ATUnityAdsExtensionInitializer</initializer>
				<finalizer>ATUnityAdsExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
	</platforms>
</extension>
