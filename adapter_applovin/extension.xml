<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.uparpu.adapter.applovin</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="default">
			<applicationDeployment>
			</applicationDeployment>
		</platform>
	    <platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>uparpuNetworkApplovin.jar</nativeLibrary>
				<initializer>com.uparpu.ane.SDK</initializer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>uparpuNetworkApplovin.jar</nativeLibrary>
				<initializer>com.uparpu.ane.SDK</initializer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libUpArpuAneApplovinAdapter.a</nativeLibrary>
				<initializer>UPArpuApplovinExtensionInitializer</initializer>
				<finalizer>UPArpuApplovinExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libUpArpuAneApplovinAdapter.a</nativeLibrary>
				<initializer>UPArpuApplovinExtensionInitializer</initializer>
				<finalizer>UPArpuApplovinExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
	</platforms>
</extension>
