<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="nend_full_board_ad_background">#FFFAFAFA</color>
    <color name="nend_full_board_ad_band_background">#90000000</color>
    <color name="nend_full_board_ad_button_background">@android:color/transparent</color>
    <color name="nend_full_board_ad_button_background_pressed">#FFE0E0E0</color>
    <color name="nend_full_board_ad_text">@android:color/black</color>
    <declare-styleable name="NendAdAttributes">
        <attr format="integer" name="NendSpotId"/>
        <attr format="string" name="NendApiKey"/>
        <attr format="boolean" name="NendReloadable"/>
        <attr format="boolean" name="NendAdjustSize"/>
        <attr format="color" name="NendTitleColor"/>
        <attr format="boolean" name="NendTitleVisible"/>
        <attr format="integer" name="NendIconCount"/>
        <attr format="boolean" name="NendIconSpaceEnabled"/>
        <attr format="enum" name="NendOrientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="NendCardView">
        <attr format="boolean" name="boardPreventCornerOverlap"/>
    </declare-styleable>
    <item format="float" name="logo_and_promo_percent_bottom" type="dimen">0.60</item>
    <item format="float" name="logo_and_promo_percent_top" type="dimen">0.50</item>
    <dimen name="max_height_rectangle_media_view_replay_cta_land">250dp</dimen>
    <dimen name="max_width_rectangle_media_view_replay_cta_port">250dp</dimen>
    <dimen name="native_video_fullscreen_card_advertiser_size">10dp</dimen>
    <dimen name="native_video_fullscreen_card_description_size">14dp</dimen>
    <dimen name="native_video_fullscreen_card_height_video_land">300dp</dimen>
    <dimen name="native_video_fullscreen_card_image_size_video_land">64dp</dimen>
    <dimen name="native_video_fullscreen_card_image_size_video_port">96dp</dimen>
    <dimen name="native_video_fullscreen_card_title_size">20dp</dimen>
    <dimen name="native_video_fullscreen_card_width_video_port">300dp</dimen>
    <item format="float" name="native_video_translucent" type="dimen">0.60</item>
    <item format="float" name="nend_full_board_ad_action_button_percent_bottom" type="dimen">0.95</item>
    <dimen name="nend_full_board_ad_card_margin_top">1dp</dimen>
    <dimen name="nend_full_board_ad_card_width_max">680dp</dimen>
    <dimen name="nend_full_board_ad_close_button_margin">4dp</dimen>
    <dimen name="nend_full_board_ad_close_button_size">40dp</dimen>
    <item format="float" name="nend_full_board_ad_content_percent_bottom" type="dimen">0.86</item>
    <item format="float" name="nend_full_board_ad_image_percent_height" type="dimen">0.44</item>
    <dimen name="nend_full_board_ad_information_offset">60dp</dimen>
    <dimen name="nend_full_board_ad_logo_margin">8dp</dimen>
    <dimen name="nend_full_board_ad_logo_size">40dp</dimen>
    <dimen name="nend_full_board_ad_promotion_text_size">20dp</dimen>
    <dimen name="nend_full_board_ad_text_size">16dp</dimen>
    <dimen name="nend_video_ad_overlay_content_size">36dp</dimen>
    <dimen name="nend_video_ad_overlay_elements_margin">8dp</dimen>
    <item name="native_video_fullscreen_card" type="id"/>
    <item name="native_video_fullscreen_card_advertiser" type="id"/>
    <item name="native_video_fullscreen_card_cta" type="id"/>
    <item name="native_video_fullscreen_card_description" type="id"/>
    <item name="native_video_fullscreen_card_rating_count" type="id"/>
    <item name="native_video_fullscreen_card_star001" type="id"/>
    <item name="native_video_fullscreen_card_star002" type="id"/>
    <item name="native_video_fullscreen_card_star003" type="id"/>
    <item name="native_video_fullscreen_card_star004" type="id"/>
    <item name="native_video_fullscreen_card_star005" type="id"/>
    <item name="native_video_fullscreen_card_title" type="id"/>
    <item name="native_video_fullscreen_videoview" type="id"/>
    <item name="nend_full_board_ad_action_button" type="id"/>
    <item name="nend_full_board_ad_card" type="id"/>
    <item name="nend_full_board_ad_close_button" type="id"/>
    <item name="nend_full_board_ad_content" type="id"/>
    <item name="nend_full_board_ad_image" type="id"/>
    <item name="nend_full_board_ad_information_button" type="id"/>
    <item name="nend_full_board_ad_information_margin_spacer" type="id"/>
    <item name="nend_full_board_ad_logo" type="id"/>
    <item name="nend_full_board_ad_margin_spacer" type="id"/>
    <item name="nend_full_board_ad_promotion" type="id"/>
    <item name="space_rectangle_media_view_replay_cta" type="id"/>
    <integer name="nend_full_board_ad_shadow_delta">4</integer>
    <string name="native_video_replay">再生する</string>
    <string name="nend_full_board_ad_action_button_text">詳細をみる</string>
    <style name="TextView.NendAd.FullBoard.ActionButton" parent="android:Widget.TextView">
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/nend_full_board_ad_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/nend_full_board_ad_text</item>
    </style>
    <style name="TextView.NendAd.FullBoard.Content" parent="android:Widget.TextView">
        <item name="android:maxLines">3</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/nend_full_board_ad_text_size</item>
        <item name="android:textColor">@color/nend_full_board_ad_text</item>
    </style>
    <style name="TextView.NendAd.FullBoard.Promotion" parent="android:Widget.TextView">
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/nend_full_board_ad_promotion_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/nend_full_board_ad_text</item>
    </style>
    <style name="Theme.NendAd.Interstitial" parent="android:Theme.Translucent">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
</resources>